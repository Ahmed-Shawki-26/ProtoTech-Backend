{
  "base": {
    "dimension_limit_cm": {
      "max_width": 38,
      "max_height": 28
    }
  },

  "panel_pricing": {
    "unit_cm2_price": {
      "≤1000": 1.6,
      "1001-1500": 1.5,
      "1501-2000": 1.4,
      "2001-2500": 1.3,
      "2501-3000": 1.2
    },
    "minimum_cm2_price": 1.2,
    "note": "Panel area is width_cm × height_cm. Pricing is capped at minimum 1.2 EGP/cm²."
  },

  "quantity_rules": {
    "≥5": 1,
    "3-4": 1.5,
    "1-2": 2,
    "note": "Multiplier applied on base price."
  },

  "deffrent_design": {
    "default": 1,
    "multiplier_formula": "price × (1 + (designs - 1) × 0.1)",
    "example": {
      "1": 1,
      "2": 1.2,
      "3": 1.3,
      "4": 1.4
    }
  },

  "delivery_format": {
    "single_pcb": 1,
    "panel_by_customer": {
      "multiplier_formula": "price × (1 + (panels - 1) × 0.1)",
      "example": {
        "1_panel": 1,
        "2_panels": 1.2,
        "3_panels": 1.3
      }
    },
    "panel_by_prototech": 1
  },

  "thickness_rules": {
    "1.0-1.6": 1,
    "0.8": 1.2,
    "0.6": 1.3,
    "0.4": 1.4,
    "2.0": 1.3
  },

  "color_rules": {
    "green": {
      "multiplier": 1,
      "extra_days": 0
    },
    "other": {
      "multiplier": 1.2,
      "extra_days": 1
    }
  },

  "high_spec_options": {
    "outer_copper_weight": {
      "1oz": 1,
      "2oz": 2.5
    },
    "min_via_hole_mm": {
      "0.3": 1,
      "<0.3": 1.3
    },
    "board_outline_tolerance_mm": {
      "0.2": 1,
      "0.1": 1.3
    }
  }
}